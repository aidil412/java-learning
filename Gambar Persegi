import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner input = new Scanner (System.in);
    int a = input.nextInt();
     for (int i = '*'; i < a; i++) {
            for (int j = '*'; j < a; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
  }
  }
}
